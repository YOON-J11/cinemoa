{{> common/header }}
<link rel="stylesheet" href="/css/mypage/mypage.css?v={{ timestamp }}" />
<div class="container mypage-container">
    <div class="page-util">
        {{ pagePath }}
    </div>
    <div class="inner-wrap">
        <div class="mypage-menu">
            <a href="/mypage"><h3>마이페이지</h3></a>
            <nav>
                <ul class="mypage-menu-ul">
                    <li><a href="/mypage/bookinglist">예매/구매내역</a></li>
                    <li>
                        <a href="/mypage/mymovie">나의 영화</a>
                        <ul class="mypage-menu-ul-sub">
                            <li><a href="/mypage/mymovie">내가 본 영화</a></li>
                            <li><a href="/mypage/myreview">내가 쓴 관람평</a></li>
                            <li><a href="/mypage/mylike">좋아요 한 영화</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="/mypage/information/myinfo">회원정보</a>
                        <ul class="mypage-menu-ul-sub">
                            <li><a href="/mypage/information/myinfo">개인정보 설정</a></li>
                            <li><a href="/mypage/information/pref">선호관람정보 설정</a></li>
                            <li><a href="/mypage/information/withdrawal">회원 탈퇴</a></li>
                        </ul>
                    </li>
                    <li><a href="/mypage/myinquiry">나의 문의내역</a></li>
                </ul>
            </nav>
        </div>
        <div class="mypage-contents">
            {{#mypageHome}}
                {{> mypage/mypageHome }}
            {{/mypageHome}}
            {{#bookinglist}}
                {{> mypage/bookinglist }}
            {{/bookinglist}}
            {{#bookingdetail}}
                {{> mypage/bookingdetail }}
            {{/bookingdetail}}
            {{#mymovie}}
                {{> mypage/mymovie }}
            {{/mymovie}}
            {{#myreview}}
                {{> mypage/myreview }}
            {{/myreview}}
            {{#mylike}}
                {{> mypage/mylike }}
            {{/mylike}}
            {{#myinfo}}
                {{> mypage/myinfo }}
            {{/myinfo}}
            {{#pref}}
                {{> mypage/pref }}
            {{/pref}}
            {{#withdrawal}}
                {{> mypage/withdrawal }}
            {{/withdrawal}}
            {{#myinquiry}}
                {{> mypage/myinquiry }}
            {{/myinquiry}}
        </div>
    </div>
</div>

{{> common/footer }}
